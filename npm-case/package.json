    "build-proto:pbjs": "pbjs --target static-module --wrap commonjs --out ./Proto.js/awesome.js ./Proto/awesome.proto",
    "build-proto:pbts": "pbts --main --out ./Proto.js/awesome.d.ts ./Proto.js/awesome.js"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "protobufjs": "^6.10.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "fs-extra": "^9.0.1",
    "typescript": "^4.1.3"
  }
}
